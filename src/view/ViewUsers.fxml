<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="650.0" prefWidth="738.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="controller.ViewUsersController">
  <!-- TODO Add Nodes -->
  <children>
    <TableView fx:id="users" layoutX="255.0" layoutY="64.0" prefHeight="473.0" prefWidth="406.0">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="158.0" text="Name" fx:id="colName" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="144.0" text="Username" fx:id="colUsername" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="104.0" text="Admin" fx:id="colAdmin" />
      </columns>
    </TableView>
    <Button fx:id="btnEdit" layoutX="93.0" layoutY="97.0" mnemonicParsing="false" onAction="#editUser" prefHeight="38.0" prefWidth="90.0" text="Edit">
      <font>
        <Font size="15.0" fx:id="x1" />
      </font>
    </Button>
    <Button fx:id="btnRemove" font="$x1" layoutX="93.0" layoutY="208.0" mnemonicParsing="false" onAction="#removeUser" prefHeight="38.0" prefWidth="90.0" text="Remove" />
    <Button fx:id="btnHome" font="$x1" layoutX="100.0" layoutY="270.0" mnemonicParsing="false" onAction="#toHome" prefHeight="31.0" prefWidth="76.0" text="Home" />
    <Button fx:id="btnAddNew" font="$x1" layoutX="93.0" layoutY="153.0" mnemonicParsing="false" onAction="#addNewUser" prefHeight="38.0" prefWidth="90.0" text="Add New" />
  </children>
</AnchorPane>