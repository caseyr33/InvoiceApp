<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane minHeight="600.0" minWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1">
   <children>
      <ListView fx:id="productListView" layoutX="44.0" layoutY="29.0" prefHeight="465.0" prefWidth="510.0" />
      <ButtonBar layoutX="201.0" layoutY="514.0" prefHeight="40.0" prefWidth="249.0">
        <buttons>
          <Button fx:id="addButton" mnemonicParsing="false" onAction="#addButtonClicked" text="Add" />
            <Button fx:id="editButton" mnemonicParsing="false" onAction="#editButtonClicked" text="Edit" />
            <Button fx:id="removeButton" mnemonicParsing="false" onAction="#removeButtonClicked" text="Remove" />
            <Button fx:id="closeButton" mnemonicParsing="false" onAction="#closeButtonClicked" text="Close" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
